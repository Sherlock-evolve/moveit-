<?xml version="1.0"?>
<robot name="my_arm_with_gripper">

  <material name="blue">
    <color rgba="0.0 0.0 0.8 1.0"/>
  </material>
  <material name="grey">
    <color rgba="0.7 0.7 0.7 1.0"/>
  </material>
  <material name="black">
    <color rgba="0.0 0.0 0.0 1.0"/>
  </material>
  <material name="white">
    <color rgba="1.0 1.0 1.0 1.0"/>
  </material>
  <material name="gripper_material">
    <color rgba="0.9 0.9 0.1 1.0"/>
  </material>

  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.06"/>
      </geometry>
      <origin xyz="0 0 0.025" rpy="0 0 0"/>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.06"/>
      </geometry>
      <origin xyz="0 0 0.025" rpy="0 0 0"/>
    </collision>
  </link>

  <joint name="shoulder_pan_joint" type="revolute">
    <parent link="base_link"/>
    <child link="shoulder_link"/>
    <origin xyz="0 0 0.05" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14" upper="3.14" effort="100" velocity="1.0"/>
  </joint>

  <link name="shoulder_link">
    <visual>
      <geometry>
        <cylinder length="0.1" radius="0.05"/>
      </geometry>
      <origin xyz="0 0 0.05" rpy="0 0 0"/>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.1" radius="0.05"/>
      </geometry>
      <origin xyz="0 0 0.05" rpy="0 0 0"/>
    </collision>
  </link>

  <joint name="shoulder_lift_joint" type="revolute">
    <parent link="shoulder_link"/>
    <child link="upper_arm_link"/>
    <origin xyz="0 0.06 0.05" rpy="1.5708 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14" upper="3.14" effort="100" velocity="1.0"/>
  </joint>

  <link name="upper_arm_link">
    <visual>
      <geometry>
        <cylinder length="0.4" radius="0.04"/>
      </geometry>
      <origin xyz="0 0.2 0" rpy="1.5708 0 0"/>
      <material name="grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.4" radius="0.04"/>
      </geometry>
      <origin xyz="0 0.2 0" rpy="1.5708 0 0"/>
    </collision>
  </link>

  <joint name="elbow_joint" type="revolute">
    <parent link="upper_arm_link"/>
    <child link="forearm_link"/>
    <origin xyz="0 0.4 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14" upper="3.14" effort="100" velocity="1.0"/>
  </joint>

  <link name="forearm_link">
    <visual>
      <geometry>
        <cylinder length="0.35" radius="0.035"/>
      </geometry>
      <origin xyz="0 0.175 0" rpy="1.5708 0 0"/>
      <material name="grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.35" radius="0.035"/>
      </geometry>
      <origin xyz="0 0.175 0" rpy="1.5708 0 0"/>
    </collision>
  </link>

  <joint name="wrist_1_joint" type="revolute">
    <parent link="forearm_link"/>
    <child link="wrist_1_link"/>
    <origin xyz="0 0.35 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14" upper="3.14" effort="50" velocity="1.0"/>
  </joint>

  <link name="wrist_1_link">
    <visual>
      <geometry>
        <cylinder length="0.08" radius="0.04"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.08" radius="0.04"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
    </collision>
  </link>

  <joint name="wrist_2_joint" type="revolute">
    <parent link="wrist_1_link"/>
    <child link="wrist_2_link"/>
    <origin xyz="0 0.05 0" rpy="-1.5708 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14" upper="3.14" effort="50" velocity="1.0"/>
  </joint>

  <link name="wrist_2_link">
    <visual>
      <geometry>
        <cylinder length="0.08" radius="0.04"/>
      </geometry>
      <origin xyz="0 0 0.04" rpy="0 0 0"/>
      <material name="grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.08" radius="0.04"/>
      </geometry>
      <origin xyz="0 0 0.04" rpy="0 0 0"/>
    </collision>
  </link>

  <joint name="wrist_3_joint" type="revolute">
    <parent link="wrist_2_link"/>
    <child link="wrist_3_link"/>
    <origin xyz="0 0 0.08" rpy="1.5708 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14" upper="3.14" effort="50" velocity="1.0"/>
  </joint>

  <link name="wrist_3_link">
    <visual>
      <geometry>
        <cylinder length="0.04" radius="0.035"/>
      </geometry>
      <origin xyz="0 0 0.02" rpy="0 0 0"/>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.04" radius="0.035"/>
      </geometry>
      <origin xyz="0 0 0.02" rpy="0 0 0"/>
    </collision>

    <visual>
      <geometry>
        <box size="0.01 0.08 0.01"/> </geometry>
      <origin xyz="0 0 0.045" rpy="0 0 0"/> <material name="white"/>
    </visual>
  </link>
  
  <joint name="wrist_3_to_gripper_base_joint" type="fixed">
    <parent link="wrist_3_link"/>
    <origin xyz="0 0 0.04" rpy="0 0 0"/> 
    <child link="gripper_base_link"/>
  </joint>

  <link name="gripper_base_link">
    <visual>
      <geometry>
        <box size="0.06 0.12 0.03"/>
      </geometry>
      <material name="gripper_material"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.06 0.12 0.03"/>
      </geometry>
    </collision>
  </link>

  <joint name="gripper_base_to_grasp_point" type="fixed">
    <parent link="gripper_base_link"/>
    <child link="grasp_point_link"/>
    <origin xyz="0 0 0.08" rpy="0 0 0"/>
  </joint>

  <link name="grasp_point_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.001"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>
  <joint name="left_finger_joint" type="prismatic">
    <parent link="gripper_base_link"/>
    <child link="left_finger_link"/>
    <origin xyz="0 0.01 0" rpy="0 0 0"/> 
    <axis xyz="0 1 0"/> 
    <limit lower="0.0" upper="0.05" effort="15" velocity="0.7"/> 
  </joint>

  <link name="left_finger_link">
    <visual>
      <geometry>
        <box size="0.02 0.02 0.08"/>
      </geometry>
      <origin xyz="0 0 0.04" rpy="0 0 0"/> 
      <material name="grey"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.02 0.02 0.08"/>
      </geometry>
      <origin xyz="0 0 0.04" rpy="0 0 0"/>
    </collision>
  </link>

  <joint name="right_finger_joint" type="prismatic">
    <parent link="gripper_base_link"/>
    <child link="right_finger_link"/>
    <origin xyz="0 -0.01 0" rpy="0 0 0"/> 
    <axis xyz="0 -1 0"/> 
    <limit lower="0.0" upper="0.05" effort="15" velocity="0.7"/> 
  </joint>

  <link name="right_finger_link">
    <visual>
      <geometry>
        <box size="0.02 0.02 0.08"/>
      </geometry>
      <origin xyz="0 0 0.04" rpy="0 0 0"/> 
      <material name="grey"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.02 0.02 0.08"/>
      </geometry>
      <origin xyz="0 0 0.04" rpy="0 0 0"/>
    </collision>
  </link>

  </robot>